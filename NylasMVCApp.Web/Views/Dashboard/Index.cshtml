
@{
    ViewBag.Title = "Dashboard";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        @if (string.IsNullOrEmpty(ViewBag.ErrorMessage))
        {
            <h2>@ViewBag.Token</h2>
        }
        else
        {
            <div class="alert alert-danger">
                <strong>Danger!</strong> @ViewBag.ErrorMessage
            </div>
        }
    </div>
</body>
</html>
